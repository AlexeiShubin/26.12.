import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        System.out.print("Введите число:");
        int a=scan.nextInt();
        if(a==0) {
            System.out.print("ноль");
        } else if (a%2==0) {
            System.out.print("Четное");
        }else{
            System.out.print("Нечетное");
        }

    }
}
