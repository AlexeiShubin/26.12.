import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        System.out.print("Введите число:");
        int t=scan.nextInt();
        if(t>-5){
            System.out.print("warm");
        } else if (t<=-5&&t>-20) {
            System.out.print("normal");
        }else if(t<=-20){
            System.out.print("cold");
        }

    }
}
